[init]
defaultBranch = main

[pull]
rebase = false

[alias]
st = status
co = checkout
cm = commit -m
cm-rename = commit --amend
cm-add = commit --amend --no-edit
a = add .
rs = reset
ucm = reset HEAD~1 --mixed
lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
