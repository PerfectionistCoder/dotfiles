prompt = ">"

padding = 160

fonts = ["Monospace"]
font_size = 32.0

[history]
decrease_interval = 48

[colors]
background = "#{{.colors.desktop.base}}e0"
prompt = "#{{.colors.desktop.text}}"
text = "#{{.colors.desktop.subtext0}}"
text_query = "#{{.colors.desktop.text}}"
text_selected = "#{{.colors.desktop.text}}"

[keybindings]
paste = ["ctrl+v"]
execute = ["Return"]
delete = ["BackSpace"]
delete_word = ["alt+BackSpace"]
complete = ["Tab"]
nav_up = ["Up"]
nav_down = ["Down"]
exit = ["Escape"]
